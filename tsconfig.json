{
	"compilerOptions": {
		"target": "es2024",
		"lib": ["es2024"],
		"moduleDetection": "force",
		"module": "preserve",
		"strict": true,
		"exactOptionalPropertyTypes": true,
		"noUncheckedIndexedAccess": true,
		"isolatedModules": true,
		"verbatimModuleSyntax": true,
		"noUncheckedSideEffectImports": true,
		"skipLibCheck": true
	}
}
